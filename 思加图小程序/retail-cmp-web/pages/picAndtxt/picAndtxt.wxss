/* pages/picAndtxt/picAndtxt.wxss */
@import "/wxParse/wxParse.wxss";

.wxParse-section {
  /* overflow: hidden; */
}
.wxParse-p text{
  font-size: 30rpx!important;
}
.wxParse-section image {
  width: 100% !important;
}
image{
  width:100%!important;
}
.trangle_title {
  width: 94rpx;
  text-align: center;
  font-size: 24rpx;
  color: #b4b4b6;
  background: url(data:image/png;base64,) 0 0 no-repeat;
  background-size: 94rpx 34rpx;
  float: left;
  height: 36rpx;
  box-sizing: border-box;
  line-height: 36rpx;
  vertical-align: middle;
}

.date_txt {
  color: #cfcfcf;
  padding: 20rpx 0 60rpx 0;
  height: 40rpx;
}

.date_txt .date {
  vertical-align: middle;
  float: right;
  width: 200rpx;
  text-align: right;
  color: #cfcfcf;
}

.head_des text {
  color: #333;
  font-size: 35rpx;
  font-weight: bold;

}

.picAndtxt {
  padding: 24rpx;
}

