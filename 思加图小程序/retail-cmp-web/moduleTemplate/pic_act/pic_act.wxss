.act_title_text {
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: left 11rpx;
  padding-left: 40rpx;
  font-size: 32rpx;
  width: 100%;
  color: #231815;
  font-weight: 500;
  background-size: 33rpx 33rpx;
  /* overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; */
  overflow : hidden;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical;
}

.pic_act_title {
  padding: 25rpx 36rpx 0;
  width: 100%;
}

.trangle_title {
  width: 94rpx;
  font-size: 24rpx;
  color: #b4b4b6;
  background: url(data:image/png;base64,) 0 0 no-repeat;
  background-size: 94rpx 34rpx;
  float: left;
  height: 36rpx;
  box-sizing: border-box;
  line-height: 35.5rpx;
  vertical-align: middle;
  text-align: center;
  padding-left: 19rpx;
  padding-bottom: 2rpx;
}

.date_txt {
  color: #cfcfcf;
  padding: 20rpx 0 70rpx 0;
}

.date_txt .date {
  vertical-align: middle;
  float: right;
  width: 200rpx;
  color: #cfcfcf;
  text-align: right;
}
