:global{
   #productsListNav{
    width: 100%;
    height: 90px;
    background: #fff;
    border-top: 1px solid #E3E3E3;
    border-bottom: 1px solid #E3E3E3;
    position: relative;
    &:after{
        content:"";
        width: 80px;
        height: 93px;
        position: absolute;
        right: 0;
        z-index: 12;
        bottom: -1px;
        background: -webkit-linear-gradient(left, rgba(230, 230, 232, 0), rgba(169, 169, 169, 1));
    };
  
    .carousel-panel{
        display: flex;

      
       li{
            flex: 1;
            text-align: center;
            color: #333333;
            font-size: 28px;
            height: 90px;
            a{
                line-height: 90px;
                display: block;
                 color: #333333;
                 font-size: 28px!important;
            }
           /* a:first-child{
                color:red;
            }
*/
            .click-a{
                color:#097cc6;
            }

        }
    }
   } 
};