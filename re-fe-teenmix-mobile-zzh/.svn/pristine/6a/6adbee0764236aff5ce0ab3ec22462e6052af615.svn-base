:global{
    #aboutus{
        width: 100%;
        height: 396px;
        padding-bottom: 10px;
        vertical-align: top;
        overflow: hidden;
        img{
            display: block;
            width: 100%;
          
            vertical-align: middle;
        }
        .leftimg{
             vertical-align: top;
            float: left;
            width: 53%;
            margin-left: 1%;
            margin-right: 1%;

        }
        .rightimg{
            vertical-align: top;
            margin-top: -1.9px;
            float: left;
            width: 43%;
            margin-left: 1%;
            margin-right: 1%;
        }
    }
};